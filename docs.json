          },
          {
            "group": "Features",
            "pages": [
              "xenovia-platform/components/agent-creation",
              "xenovia-platform/components/agent-execution",
              "xenovia-platform/components/analytics",
              "xenovia-platform/components/collaboration",
              "xenovia-platform/components/dashboard",
              "xenovia-platform/components/deployment",
              "xenovia-platform/components/fine-tuning",
              "xenovia-platform/components/integrations",
              "xenovia-platform/components/knowledge-management",
              "xenovia-platform/components/marketplace",
              "xenovia-platform/components/memory-management",
              "xenovia-platform/components/prompts",
              "xenovia-platform/components/security",
              "xenovia-platform/components/testing",
              "xenovia-platform/components/tools"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication"
            ]
          },
          {
            "group": "Endpoint Examples",
            "openapi": {
              "source": "https://carbon-api-806155479073.us-central1.run.app/api/schema/",
              "directory": "api-reference"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://xenovia.io",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/xenovia-light.png",
    "dark": "/logo/xenovia-dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:richard.loomis@xenovia.com"
      }
